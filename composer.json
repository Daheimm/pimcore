{
  "name": "pimcore/skeleton",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "process-timeout": 0,
    "allow-plugins": {
      "symfony/runtime": true
    }
  },
  "prefer-stable": true,
  "require": {
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2.18",
    "guzzlehttp/guzzle": "^7.8",
    "guzzlehttp/promises": "^2.0",
    "jms/serializer-bundle": "^5.4",
    "monolog/monolog": "*",
    "php-amqplib/rabbitmq-bundle": "^2.13",
    "pimcore/admin-ui-classic-bundle": "^1.3",
    "pimcore/customer-management-framework-bundle": "^4.0",
    "pimcore/data-hub": "^1.6",
    "pimcore/data-importer": "^1.8",
    "pimcore/ecommerce-framework-bundle": "^1.0",
    "pimcore/perspective-editor": "^1.7",
    "pimcore/pimcore": "^11.0.4",
    "pimcore/web2print-tools-bundle": "^5.0",
    "symfony/amqp-messenger": "^7.0",
    "symfony/dotenv": "^6.2",
    "symfony/http-client": "^7.0",
    "symfony/messenger": "^6.4",
    "symfony/monolog-bridge": "^7.0",
    "symfony/orm-pack": "^2.4",
    "symfony/runtime": "^6.2",
    "symfony/serializer": "^6.4"
  },
  "require-dev": {
    "codeception/codeception": "^5.0.3",
    "codeception/module-symfony": "^3.1.0",
    "friendsofphp/php-cs-fixer": "^3.51",
    "phpstan/phpstan": "^1.10",
    "symfony/maker-bundle": "^1.54"
  },
  "suggest": {
    "pimcore/data-hub": "Universal data interface for GraphQL, CSV and other formats"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Pimcore\\Model\\DataObject\\": "var/classes/DataObject"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
"repositories": [
    {
	"type": "path",
        "url": "./packages/set-st/pimcore_bridge-up",
        "options": {
            "symlink": true
        }
    }
],
  "scripts": {
    "post-create-project-cmd": "Pimcore\\Composer::postCreateProject",
    "post-install-cmd": [
      "Pimcore\\Composer::postInstall",
      "@pimcore-scripts"
    ],
    "post-update-cmd": [
      "Pimcore\\Composer::postUpdate",
      "@pimcore-scripts"
    ],
    "pimcore-scripts": [
      "Pimcore\\Composer::clearCache",
      "Pimcore\\Composer::installAssets"
    ]
  }
}
